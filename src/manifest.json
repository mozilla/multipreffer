{
  "manifest_version": 2,
  "name": "Multipreffer",
  "version": "1.0",
  "description": "Flippa lotta prefs",
  "background": {
    "scripts": ["background.js"]
  },
  "applications": {
    "gecko": {
      "id": "multipreffer@shield.mozilla.org",
      "strict_min_version": "64.0"
    }
  },

  "permissions": [],

  "experiment_apis": {
    "study": {
      "schema": "./privileged/study/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/study/api.js",
        "paths": [["study"]]
      }
    },
    "firefoxhooks": {
      "schema": "./privileged/firefoxhooks/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/firefoxhooks/api.js",
        "paths": [["firefoxhooks"]]
      }
    }
  }
}
